<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.introact">
    <!-- permission -->
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VegetableFruitImage"></activity>
        <activity
            android:name=".LoadCalorie"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".calorie_protein"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".LoadData"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".calorie_recipe_option" />
        <activity
            android:name=".RecipeDetail"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".NavAct"
            android:theme="@style/Theme.NavActivityDemo.NoActionBar"/>
        <activity android:name=".fruit_list" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".Splash"
            android:theme="@style/Theme.NavActivityDemo.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FruitImage"
            android:theme="@style/Theme.NavActivityDemo.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Theme.NavActivityDemo.NoActionBar" />
        <activity
            android:name=".profile"
            android:theme="@style/Theme.NavActivityDemo.NoActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>